{
  "name": "vue-paint",
  "version": "0.5.1",
  "private": false,
  "type": "module",
  "author": "Robert Rosman",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/robertrosman/vue-paint.git"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/vue-paint.umd.js",
  "module": "./dist/vue-paint.js",
  "types": "./dist/main.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/main.d.ts",
        "default": "./dist/vue-paint.js"
      },
      "require": {
        "types": "./dist/main.d.ts",
        "default": "./dist/vue-paint.umd.cjs"
      }
    },
    "./themes/*": {
      "import": "./dist/themes/*",
      "require": "./dist/themes/*"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build:lib {@}\" --",
    "preview": "vite preview",
    "test:unit": "vitest --typecheck",
    "build:lib": "vite build --config vite.lib-config.ts",
    "build:demo": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "@luncheon/simplify-svg-path": "^0.2.0",
    "@vueuse/core": "^10.9.0",
    "@vueuse/math": "^10.10.0",
    "animejs": "^3.2.2",
    "vue": "^3.4.21"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.8.0",
    "@tsconfig/node20": "^20.1.4",
    "@types/animejs": "^3.1.12",
    "@types/jsdom": "^21.1.6",
    "@types/node": "^20.12.5",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/test-utils": "^2.4.5",
    "@vue/tsconfig": "^0.5.1",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.23.0",
    "jsdom": "^24.0.0",
    "npm-run-all2": "^6.1.2",
    "rollup-plugin-visualizer": "^5.12.0",
    "typescript": "~5.4.0",
    "vite": "^5.2.8",
    "vite-plugin-dts": "^3.9.1",
    "vitest": "^1.4.0",
    "vue-tsc": "^2.0.11"
  }
}